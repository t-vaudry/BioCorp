            <tr><td>S</td><td>GC</td><td><b>S</b>trong</td></tr>
            <tr><td>W</td><td>AT</td><td><b>W</b>eak</td></tr>
            <tr><td>B</td><td>CGT</td><td>Not A</td></tr>
            <tr><td>D</td><td>AGT</td><td>Not C</td></tr>
            <tr><td>H</td><td>ACT</td><td>Not G</td></tr>
            <tr><td>V</td><td>ACG</td><td>Not T</td></tr>
